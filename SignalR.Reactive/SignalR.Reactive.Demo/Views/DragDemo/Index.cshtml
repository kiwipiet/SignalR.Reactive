<div style="background: grey; height: 200px; width: 200px;" id="shape"></div>

<script type="text/javascript" src="/signalr/hubs"></script>
<script type="text/javascript">

    $(function() {
        var myHub = $.connection.rxHub,
            $shape = $('#shape');

        myHub.server.observe('ShapeMoved')
            .subscribe(function(data) {
                if ($.connection.hub.id !== data.Cid) {
                    $shape.css({ left: data.X, top: data.Y });
                }
            });

        $.connection.hub.start()
            .done(function() {
                $shape.draggable({
                    drag: function() {
                        myHub.server.moveShape(this.offsetLeft, this.offsetTop || 0);
                    }
                });
            });

    });
</script>